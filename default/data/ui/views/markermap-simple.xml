<dashboard script="autodiscover.js">
    <label>Simple marker map</label>
    <row>
        <panel>
            <search id="heremaps_search">
                <query><![CDATA[
                    |inputlookup earthquake.csv| rename mag as value
                ]]></query>
                <earliest>$earliest$</earliest>
                <latest>$latest$</latest>
            </search>
            <html>
                <div id="map"
                     class="splunk-view"
                     data-require="app/heremaps/heremaps/heremarkermap"
                     data-options='{
                            "managerid": "heremaps_search",
                            "height": "400px"
                         }'>
                </div>
            </html>
        </panel>
    </row>
</dashboard>